{
    "name": "<%=title%>",
    "version": "<%=version%>",
    "description": "<%=description%>",
    "private": true,
    "scripts": {
        "start": "set NODE_ENV=development&&webpack-dev-server",
        "build": "set NODE_ENV=production&&webpack --progress --colors",
        "daily": "set NODE_ENV=daily&&webpack --progress --colors --watch",
        "deploy":"http-server ./dist -p 80",
        "fix": "eslint --fix src/**/*.js -o eslint-report.html -f html",
        "zip": "dist-zip -v",
        "clean": "npm run zip&&rimraf dist",
        "pub": "rimraf dist & npm run build & npm run zip",
        "precommit": "lint-staged"
    },
    "lint-staged": {
        "src/**/*.js": [
            "eslint --fix",
            "git add"
        ]
    },
    "devDependencies": {
        "@babel/core": "^7.0.0-beta.51",
        "@babel/preset-env": "7.0.0-beta.51",
        "@babel/plugin-proposal-class-properties": "^7.0.0-beta.51",
        "babel-eslint": "^9.0.0",
        "babel-loader": "^8.0.0-beta.0",  
        "copy-webpack-plugin": "^4.5.1",
        "css-loader": "^0.28.11",
        "dist-zip": ">1.3.0",
        "es3ify-webpack-plugin": "^0.0.1",
        "eslint": "4.19.1",
        "eslint-config-airbnb": "17.0.0",
        "eslint-loader": "^2.0.0",
        "eslint-plugin-import": "^2.13.0",
        "eslint-plugin-jsx-a11y": "^6.0.3",
        "eslint-plugin-react": "^7.10.0",
        "extract-text-webpack-plugin": "^4.0.0-beta.0",
        "file-loader": "^1.1.11",
        "html-loader": "^0.5.5",
        "html-webpack-check-source-plugin": ">1.0.0",
        "html-webpack-insert-script-plugin": ">1.3.3",
        "html-webpack-plugin": "^3.2.0",
        "http-server": "^0.11.1",
        "husky": "^0.14.3",
        "image-webpack-loader": "^4.3.1",
        "lint-staged": "^7.2.0",
        "node-sass": "^4.9.0",
        "postcss-assets": "^5.0.0",
        "postcss-loader": "^2.1.5",
        "rimraf": "^2.6.2",
        "sass-loader": "^7.0.3",
        "style-loader": "^0.21.0",
        "uglifyjs-webpack-plugin": "^1.2.6",
        "url-loader": "^1.0.1",
        "webpack": "^4.10.2",
        "webpack-cli": "^3.0.8",
        "webpack-dev-server": "^3.1.4",
        "webpack-spritesmith": "^0.4.1"
    },
    "engines": {
        "node": ">=8.0.0"
    },
    "author": "ZuoWenQi",
    "person":{
        "name":"<%=developer%>"
    },
    "license": "ISC",
    "dependencies": {
        "component-ie": "^1.0.0"
    }
}